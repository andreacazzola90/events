{
  "name": "event-scanner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:vercel": "npm run check-env && prisma generate && prisma db push --accept-data-loss && npm run seed && next build",
    "postinstall": "npm run generate-safe",
    "generate-safe": "DATABASE_URL=${DATABASE_URL:-postgresql://placeholder} prisma generate",
    "check-env": "node scripts/check-env.js",
    "start": "next start",
    "lint": "eslint",
    "seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run seed",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "test:browser": "tsx scripts/test-browser.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@sparticuz/chromium": "^141.0.0",
    "@sparticuz/chromium-min": "^131.0.1",
    "@supabase/supabase-js": "^2.79.0",
    "@types/bcrypt": "^6.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/tesseract.js": "^0.0.2",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.4",
    "groq-sdk": "^0.34.0",
    "leaflet": "^1.9.4",
    "next": "^16.0.1",
    "next-auth": "^4.24.13",
    "openai": "^6.7.0",
    "playwright-chromium": "^1.56.1",
    "prisma": "^6.18.0",
    "puppeteer-core": "^21.11.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-leaflet": "^5.0.0",
    "tesseract.js": "^4.1.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.6.2",
    "typescript": "^5"
  }
}
